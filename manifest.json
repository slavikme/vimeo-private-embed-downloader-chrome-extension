{
    "manifest_version": 2,
    "name": "Vimeo Private Embed Downloader",
    "version": "1.0.0",
    "background": {
        "scripts": [
            "src/DataRetriever.js",
            "src/DataStore.js",
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "<all_urls>",
        "tabs",
        "downloads",
        "webNavigation"
    ],
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "icon_16.png",
            "24": "icon_24.png",
            "32": "icon_32.png",
            "64": "icon_64.png"
        },
        "default_title": "Download Video",
        "default_popup": "popup.html"
    }
}